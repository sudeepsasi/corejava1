package com.vivekinfo.wrapperdemo;

public class string {

	public static void main(String[] args) {
	
	
		String s1 = new String ("Hari");
		
	String i1 = s1.toUpperCase();
	String i2 = s1.substring(1);
	int i3 = s1.length();
	boolean i4 = s1.equals("Hari");
	int i5 = s1.lastIndexOf("Hari");
	System.out.println("show :"+String.valueOf(i1));
	System.out.println("remaing word:"+i2);
	System.out.println("length:"+i3);
	System.out.println("statement:"+i4);
	System.out.println("index:"+i5);
	
	
	
	
	
	
	
	
	
			

	}
	
}
