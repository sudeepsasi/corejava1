package project;
//class
public class SumofNnumbers {
	//fields
	private  int i, n,sum=0;
//constructor
	public SumofNnumbers(int n){
		this.n = n;
		
//methods
		public void calculation(){
			
		for(i=0;i<=n;i++)
		{
			sum = sum+i;
			System.out.println("print the value:"+sum);
		}
			
			
		}
		
	}
	

}
