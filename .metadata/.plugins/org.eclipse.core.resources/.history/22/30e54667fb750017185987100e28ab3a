package today28;
//class
public class College {
	//fields
private	int Id;
private	String Name;
//constructor
public College(int Id,String Name){
	this.Id = Id;
	this.Name = Name;
}
//methods
public void studentdetails(){
	System.out.println("Student Id:"+Id);
	System.out.println("Student Name:"+Name);	
}


	
}



